{
  "version": 2,
  "builds": [
    {
      "src": "dist/web/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/web/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/web/server/index.js"
    },
    {
      "src": "/health",
      "dest": "dist/web/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/web/client/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "public": true
} 