{
  "version": 2,
  "builds": [
    {
      "src": "dist/web/server/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/web/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/web/server/vercel.js"
    },
    {
      "src": "/health",
      "dest": "dist/web/server/vercel.js"
    },
    {
      "src": "/js/(.*)",
      "dest": "dist/web/client/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "dist/web/client/css/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "dist/web/client/img/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/web/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/web/client/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "public": true
} 