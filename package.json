{
  "name": "neutral-app",
  "version": "1.0.0",
  "description": "A domain-agnostic, ultra-modular application shell",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:client": "vite build",
    "build:full": "npm run build && npm run build:client",
    "build:production": "ts-node scripts/build-production.ts --env=production --optimize",
    "build:staging": "ts-node scripts/build-production.ts --env=staging --optimize",
    "build:analyze": "ts-node scripts/build-production.ts --env=production --optimize --analyze",
    "validate:env": "ts-node scripts/validate-env.ts",
    "docker:build": "docker build -t neutral-app .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "health-check": "./scripts/health-check.sh",
    "vercel:deploy": "vercel --prod",
    "vercel:dev": "vercel dev",
    "vercel:build": "npm run build:full && vercel build",
    "monitor:health": "./scripts/health-check.sh",
    "monitor:performance": "node -e \"console.log('Performance monitoring not yet implemented')\"",
    "monitor:logs": "node -e \"console.log('Log monitoring not yet implemented')\"",
    "verify:deployment": "./scripts/verify-deployment.sh",
    "verify:performance": "./scripts/verify-performance.sh",
    "verify:security": "./scripts/verify-security.sh",
    "verify:all": "./scripts/verify-deployment.sh && ./scripts/verify-performance.sh && ./scripts/verify-security.sh",
    "rollback": "./scripts/rollback.sh",
    "smoke:tests": "./scripts/smoke-tests.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unified": "ts-node scripts/run-unified-tests.ts",
    "test:all": "npm run test:unified",
    "quality-gates": "ts-node scripts/run-quality-gates.ts",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test tests/e2e/visual-regression.spec.ts",
    "test:visual:update": "playwright test tests/e2e/visual-regression.spec.ts --update-snapshots",
    "dev": "ts-node src/web/server/index.ts",
    "dev:client": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "dev:build": "npm run build && node dist/web/server/index.js",
    "start": "node dist/web/server/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "mcp": "npx @playwright/mcp@latest --port 8931"
  },
  "keywords": [
    "modular",
    "plugin",
    "application-shell"
  ],
  "author": "NeutralApp Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/mcp": "^0.0.32",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "next-auth": "^4.24.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  }
}
