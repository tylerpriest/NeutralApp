{
  "name": "neutral-app",
  "version": "1.0.0",
  "description": "A domain-agnostic, ultra-modular application shell",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:client": "vite build",
    "build:full": "npm run build && npm run build:client",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "ts-node src/web/server/index.ts",
    "dev:client": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "dev:build": "npm run build && node dist/web/server/index.js",
    "start": "node dist/web/server/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "modular",
    "plugin",
    "application-shell"
  ],
  "author": "NeutralApp Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "@types/cors": "^2.8.19",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  }
}
