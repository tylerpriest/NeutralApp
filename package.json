{
  "name": "neutral-app",
  "version": "1.0.0",
  "description": "A domain-agnostic, ultra-modular application shell",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:client",
    "build:server": "tsc",
    "build:client": "rm -rf dist/web/client && vite build",
    "vercel-build": "npm run build:full",
    "build:full": "npm run build:server && npm run build:client",
    "build:production": "ts-node scripts/build-production.ts --env=production --optimize",
    "build:staging": "ts-node scripts/build-production.ts --env=staging --optimize",
    "build:analyze": "ts-node scripts/build-production.ts --env=production --optimize --analyze",
    "validate:env": "ts-node scripts/validate-env.ts",
    "docker:build": "docker build -t neutral-app .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "health-check": "./scripts/health-check.sh",
    "monitor:health": "./scripts/health-check.sh",
    "monitor:performance": "node -e \"console.log('Performance monitoring not yet implemented')\"",
    "monitor:logs": "node -e \"console.log('Log monitoring not yet implemented')\"",
    "verify:deployment": "./scripts/verify-deployment.sh",
    "verify:performance": "./scripts/verify-performance.sh",
    "verify:security": "./scripts/verify-security.sh",
    "verify:all": "./scripts/verify-deployment.sh && ./scripts/verify-performance.sh && ./scripts/verify-security.sh",
    "rollback": "./scripts/rollback.sh",
    "smoke:tests": "./scripts/smoke-tests.sh",
    "test": "jest --config jest.config.mjs --testTimeout=10000 --maxWorkers=1 --bail=1 --forceExit --detectOpenHandles",
    "test:watch": "jest --config jest.config.mjs --watch --testTimeout=10000 --maxWorkers=1 --detectOpenHandles",
    "test:coverage": "jest --config jest.config.mjs --coverage --testTimeout=10000 --maxWorkers=1 --forceExit --detectOpenHandles",
    "test:unified": "ts-node scripts/run-unified-tests.ts",
    "test:all": "npm run test:unified",
    "quality-gates": "ts-node scripts/run-quality-gates.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test tests/e2e/visual-regression.spec.ts",
    "test:visual:update": "playwright test tests/e2e/visual-regression.spec.ts --update-snapshots",
    "dev": "npm run build:server && node dist/web/server/index.js",
    "dev:client": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "dev:build": "npm run build && node dist/web/server/index.js",
    "start": "node dist/web/server/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "mcp": "npx @playwright/mcp@latest --port 8931"
  },
  "keywords": [
    "modular",
    "plugin",
    "application-shell"
  ],
  "author": "NeutralApp Team",
  "license": "MIT",
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@playwright/mcp": "^0.0.32",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vercel/node": "^5.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.526.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1"
  }
}
